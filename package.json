{
  "name": "parse-changelog-action",
  "version": "1.2.0",
  "description": "This GitHub action parses a specific release from a CHANGELOG that roughly follows the \"Keep a Changelog\" format.",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "GitHub",
    "action",
    "parse",
    "changelog"
  ],
  "author": "Marcel Schwarz",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "husky": "^8.0.3"
  }
}
